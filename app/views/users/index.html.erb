<div class="container mt-3">
  <div class="row">
    <div class="col-12">
      <h1>All our users</h1>
    </div>
  </div>

  <div class="row">
    <% @users.each do |user| %>
      <div class="col-md-4">
        <div class="user-card">
          <div class="user-card-content">
            <div class="user-card-title">
              <h4><%= user.nickname %></h4>
            </div>
            <p><%= user.preferred_date_ideas %></p>
            <div class="flex">
              <div class="flex">
              </div>
              <div class="flex ms-3">
              </div>
            </div>
          </div>
          <div class="fairy-card-actions">
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
