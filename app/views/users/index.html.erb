<div class="container mt-3">
  <div class="row">
    <div class="col-12">
      <h1>All our Users</h1>

      <ul>
        <% @users.each do |user| %>
          <%= cl_image_tag user.photo.key, height: 300, width: 400, crop: :fill %>
          <li><%= user.name %></li>
          <li><%= user.preferred_date_ideas %></li>
          <li><%= user.age %></li>
          <% end %>
      </ul>
    </div>
  </div>
