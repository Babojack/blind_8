<div class="container-registration">
  <%= image_tag "couple.png", :class => "medium-logo" %>
</div>

<div class="index-container index-container-padding">
  <% @users.each do |user| %>
    <%= link_to user do %>
      <div class="card-product">
        <div class="container-card-image">
          <%= cl_image_tag user.photo.key, crop: :fill%>
          <div class="card-product-infos card-product-infos-index">
            <h2><%= user.nickname.capitalize %>, <%= user.age %></h2>
            <p><%= user.preferred_date_ideas %></p>
            <p id="zodiac"><%= user.zodiac %></p>
          </div>
        </div>
        <%= link_to "poke", chatrooms_path(user_a_id: user.id), class: "btn btn-flat btn-sign-up", id: "poke", method: :create %>
      </div>
    <% end %>
  <% end %>
</div>
